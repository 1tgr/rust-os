include_rules

RUSTFLAGS += --cfg feature=\"no_std\"
RUSTFLAGS += --extern core=../libcore/libcore.rlib
RUSTFLAGS += --extern std=../libstd/libstd.rlib
RUSTFLAGS += -L ../liballoc
RUSTFLAGS += -L ../libcollections
RUSTFLAGS += -L ../liblibc
RUSTFLAGS += -L ../libcore
RUSTFLAGS += -L ../librustc_unicode

: bit-vec/src/lib.rs | ../libstd/libstd.rlib bit-vec/src/*.rs |> !RUSTC_LIB |> libbit-vec.rlib
: spinlock-rs/src/lib.rs | ../libcore/libcore.rlib spinlock-rs/src/*.rs |> !RUSTC_LIB |> libspin.rlib
: lazy-static.rs/src/lib.rs | ../libcore/libcore.rlib libspin.rlib lazy-static.rs/src/*.rs |> !RUSTC_LIB --extern spin=libspin.rlib |> liblazy-static.rlib
: bitflags/src/lib.rs | ../libstd/libstd.rlib bitflags/src/*.rs |> !RUSTC_LIB --crate-name=bitflags --extern std=../libstd/libstd.rlib |> libbitflags.rlib

